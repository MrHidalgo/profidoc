include ../_mixin/mixin

-
    var arrFontName = [
        'https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800'
    ];

    var arrStyleName = [
        './style/style.min.css',
        './style/security/security.min.css'
    ];

    var scriptName = [
        './script/jquery.min.js',
        './script/script.min.js'
    ];

doctype html
html
    head
        +htmlTemplateHEAD('Безопасность', arrFontName, arrStyleName, false)

    body

        include ../_template/_dop/nav

        // CONTENT BLOCK
        block content
            include ../_template/security/content

        include ../_template/_dop/footer

        +htmlTemplateFOOT(scriptName)