include ../_mixin/mixin

-
    var arrFontName = [
        'https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800'
    ];

    var arrStyleName = [
        './style/style.min.css',
        './style/check-in.min.css'
    ]

    var scriptName = [
        './script/jquery.min.js',
        './script/script.min.js'
    ];




doctype html
html
    head
        +htmlTemplateHEAD('Реєстрація ТОВ', arrFontName, arrStyleName, false)

    body.internal

        //- NAVIGATION MENU
        include ../_template/_dop/nav

        .wrap-container
            block sign
                include ../_template/main/form-sign

            //- BREADCRUMBS BLOCK
            block breadcrumbs
                include ../_template/check-in/breadcrumbs


            block info
                include ../_template/check-in/info

            block choose
                include ../_template/check-in/choose


        //- FOOTER MENU
        include ../_template/_dop/footer

        +htmlTemplateFOOT(scriptName)