include ../_mixin/mixin

-
    var arrFontName = [
        'https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800'
    ];

    var arrStyleName = [
        './style/style.min.css',
        './style/contracts-designer.min.css'
    ]

    var scriptName = [
        './script/jquery.min.js',
        './script/script.min.js'
    ];




doctype html
html
    head
        +htmlTemplateHEAD('Конструктор договоров', arrFontName, arrStyleName, false)

    body.internal

        // NAVIGATION MENU
        include ../_template/_dop/nav

        .wrap-container
            block sign
                include ../_template/main/form-sign

            // BREADCRUMBS BLOCK
            block breadcrumbs
                include ../_template/contracts-designer/breadcrumbs


            // VIDEO SUBSCRIBE
            include ../_template/contracts-designer/video-sub

            // CREATE CONTRACTS
            include ../_template/contracts-designer/create

            // CHOOSE CONTRACTS
            include ../_template/contracts-designer/choose

            // BENEFITS CONTRACTS
            include ../_template/contracts-designer/benefits


        // FOOTER MENU
        include ../_template/_dop/footer

        +htmlTemplateFOOT(scriptName)