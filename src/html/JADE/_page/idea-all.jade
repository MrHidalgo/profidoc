include ../_mixin/mixin

-
    var arrFontName = [
        'https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800'
    ];

    var arrStyleName = [
        '../style/style.min.css',
        '../style/_dop/breadcrumbs.min.css',
        '../style/idea-all/idea-all.min.css',
        '../style/idea-all/filter.min.css',
        '../style/idea-all/content.min.css'
    ]

    var scriptName = [
        '../script/jquery.min.js',
        '../script/script.min.js'
    ];





doctype html
html
    head
        +htmlTemplateHEAD('', arrFontName, arrStyleName, false)

    body

        // NAVIGATION MENU
        include ../_template/_dop/nav

        // BREADCRUMBS BLOCK
        block breadcrumbs
            include ../_template/idea-all/breadcrumbs


        block filter
            include ../_template/idea-all/filter

        block content
            include ../_template/idea-all/content


        // FOOTER MENU
        include ../_template/_dop/footer

        +htmlTemplateFOOT(scriptName)